def amp(a,b,c,d):
    Zf=[]
    df=[]
    for x in a:
        for y in b:
              for z in c:
                   Zf.append(x*y*z)
    de=sum(Zf)
    for k in d:
        df.append(de*k)
    ds=sum(df)

    return ds




intwin=[4,5,6]
